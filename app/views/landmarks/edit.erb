<h1>Edit <%= @landmark.name %> </h1>
<hr />
<form method="POST" action="/landmarks/<%=@landmark.id%>">
  <p>

     <label>Landmark Name: </label><input type="text" id="name" name="landmark[name]" value="<%=@landmark.name%>" /></p>
  <p>


      <label>Landmark Year Completed: </label><input type="text" id="year_completed" name="landmark[year_completed]" value="<%=@landmark.year_completed%>"/></p>

      <input type="submit" value="Edit Landmark" />
  </form>
